<!DOCTYPE html>
<html>
	<head>
		<meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
		<!--[if lt IE 9]>
			<script type="text/javascript">
				window.location = "/error/notsupported";
			</script>
		<![endif]-->
		<title>
			表单制作
		</title>
		
		<link href="index.css" type="text/css" rel="stylesheet">
	</head>
	<body>
		
				<div id="fx-frame-west">
					<div class="frame-inner-list">
						<div class="widget-cate">
							基础字段
						</div>
						<ul>
							<li class="form-edit-widget-label" xtype="text">
								<a>
									<i class="icon-widget-text">
									</i>
									<span>
										单行文本
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="textarea">
								<a>
									<i class="icon-widget-textarea">
									</i>
									<span>
										多行文本
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="number">
								<a>
									<i class="icon-widget-number">
									</i>
									<span>
										数字
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="datetime">
								<a>
									<i class="icon-widget-datetime">
									</i>
									<span>
										日期时间
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="radiogroup">
								<a>
									<i class="icon-widget-radiogroup">
									</i>
									<span>
										单选按钮组
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="checkboxgroup">
								<a>
									<i class="icon-widget-checkboxgroup">
									</i>
									<span>
										复选框组
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="combo">
								<a>
									<i class="icon-widget-combo">
									</i>
									<span>
										下拉框
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="combocheck">
								<a>
									<i class="icon-widget-combocheck">
									</i>
									<span>
										下拉复选框
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="separator">
								<a>
									<i class="icon-widget-separator">
									</i>
									<span>
										分割线
									</span>
								</a>
							</li>
						</ul>
						<div class="widget-cate">
							增强字段
						</div>
						<ul>
							<li class="form-edit-widget-label" xtype="address">
								<a>
									<i class="icon-widget-address">
									</i>
									<span>
										地址
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="location">
								<a>
									<i class="icon-widget-location">
									</i>
									<span>
										定位
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="upload">
								<a>
									<i class="icon-widget-upload">
									</i>
									<span>
										文件上传
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="subform">
								<a>
									<i class="icon-widget-subform">
									</i>
									<span>
										子表单
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="linkquery">
								<a>
									<i class="icon-widget-linkquery">
									</i>
									<span>
										关联查询
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="linkdata">
								<a>
									<i class="icon-widget-linkdata">
									</i>
									<span>
										关联数据
									</span>
								</a>
							</li>
						</ul>
						<div class="widget-cate">
							部门成员字段
						</div>
						<ul>
							<li class="form-edit-widget-label" xtype="user">
								<a>
									<i class="icon-widget-user">
									</i>
									<span>
										成员单选
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="usergroup">
								<a>
									<i class="icon-widget-usergroup">
									</i>
									<span>
										成员多选
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="dept">
								<a>
									<i class="icon-widget-dept">
									</i>
									<span>
										部门单选
									</span>
								</a>
							</li>
							<li class="form-edit-widget-label" xtype="deptgroup">
								<a>
									<i class="icon-widget-deptgroup">
									</i>
									<span>
										部门多选
									</span>
								</a>
							</li>
						</ul>
					</div>
				</div>
				
			
	</body>

</html>